/*
        |----[-2ohm+]----(c)---[-2v+]------|
        |                                  |
    |--(a)---------------[-2ohm+]---------(b)
    |   |                                  |
    +   +                                  +
   1A  3ohm                               4ohm
    -   -                                  -
    |   |                                  |
    |---|----------------------------------|
        |
       gnd
*/
Ia = 1A
Ua = 2V
Ra = 2
Rb = 2
Rc = 3
Rd = 4
pina{
	Ra(-)
	Rb(-)
	Rc(+)
	Ia(+)
}
pinb{
	Ua(+)
	Rb(+)
	Rd(+)
}
pinc{
	Ra(+)
	Ua(-)
}
gnd{
	Ia(-)
	Rc(-)
	Rd(-)
}
