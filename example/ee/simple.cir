/*
 |----------------pin1----------------|
V1+                                  R1+
V1-                                  R1-
 |pin3------------S(G)D-----------pin2|
                    |
                  pin4
V1{
	+@pin1
	-@pin3
}
R1{
	+@pin1
	-@pin2
}
NMOS1{
	D@pin2
	S@pin3
	G@pin4
}
*/
/*                    (bulk-boost)
|--------------------------------------------------------|
|   pin1   pin2    pin3                 pin4             |
|    |      |       |                    |               |
|    |----D(G)S-----|------+DIODE--------|-----------|   |
|    |              |                    |           |   |
|   V1+            L1+                  C1+         R1+  |
|   V1-            L1-                  C1-         R1-  |
|    |              |                    |           |   |
|    |------------------------|----------------------|   |
|                             |                          |
|                            pin5                        |
|--------------------------------------------------------|
*/




pin1{
	V1(+)
	NMOS1(D)
}
pin2{
	NMOS1(G)
}
pin3{
	NMOS1(S)
	DIODE(+)
	L1(+)
}
pin4{
	DIODE(-)
	C1(+)
	R1(+)
}
pin5{
	V1(-)
	L1(-)
	C1(-)
	R1(-)
}